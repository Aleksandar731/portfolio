function jedan() {  imamo funkciju jedan
    return fetch("/users/pera") saljemo zahtev serveru sa putanjom users folder pera fajl
      .then(function(response) { ovo se izvrsava ako je prethodna linija gotova ako je kompijuter nasao putanju response
        pretstavlja odgovor servera i sadrzi razne informacije sa servera 
        console.log(response); stampamo u konzoli te informacije sa servera 
      })
      .catch(function(err) { ukoliko se desila greska u prethodnim thenovima istampace gresku
        console.log("Nažalost, imamo problem: " + err); stampanje greske 
      });
  }
  

  fetch('https://api.example.com/data') -- KORISTIMO FETCH DA BISMO POSLALI HTTP GET ZAHTEV SERVERU PUTANJA KA SERVERU JE LINK URL
  .then(response => response.json()) -- KORISTIMO THEN LANAC KAKO BI SMO RUKOVALI REZULTATOM KOJ JE VRACEN OD FETCH FUNKCIJE I U RESPONSE JE SMESTEN TAJ ODGOVOR I .JSON
  .then(data => { --KORISTI SE ZA RUKOVANJE PODACIMA KOJI SU DOBIJENI IZ ODGOVORA SERVERA I KOJI SU PREVEDENI IZ JSON U JS OBJEKTE A DATA PRETSTAVLJA TE PODATKE I ZELIMO DA UNUTAR TOGA ISPISEMO U KONZOLI TE PREVEDENE PODATKE  
    console.log(data); 
  })
  .catch(error => { -- KORISTI SE CATCH ZA RUKOVANJE GRESKAMA UKOLIKO JE DOSLO DO GRESKE U LANCIMA THEN I U PROSLOM KODU I UNUTAR NJE SE ISPISUJE EROR SA SADRZAJEM GRESKE 
    console.error('Došlo je do greške:', error);
  });
